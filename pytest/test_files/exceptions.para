typedef struct CustomError {
    string error_msg;
    string display_name;
} CustomError;

entry status main()
{
    CustomError e = {
        .error_msg="Something bad",
        .display_name="Error"
    };

    try
    {
        raise e;
    }
    except (Exception, Exception2) as e
    {
        print(e);
    }
    finally
    {
        print("finally");
    }
    else
    {
        print("else");
    }
}